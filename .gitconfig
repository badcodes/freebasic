[remote "github"]
	url = git@github.com:eotect/freebasic.git
	fetch = +refs/heads/*:refs/remotes/github/*
[remote "gitorious"]
	url = git@gitorious.org:eotect/freebasic.git
	fetch = +refs/heads/*:refs/remotes/gitorious/*
